}
.center{
	display: flex;
    position: relative;
    width: 520px;
    background-color: rgb(250, 251, 252);
    flex-direction: column;
}
.content{
	display: flex;
    flex-direction: column;
    transition: height 300ms ease 0s;
}
.one{
	box-sizing: border-box;
    direction: ltr;
    height: auto;
    position: relative;
    width: 520px;
    will-change: transform;
    overflow: auto;
    outline: none;
    touch-action: manipulation;
}
.two{
	width: auto;
    height: 4359px;
    max-width: 520px;
    max-height: 4359px;
    overflow: hidden;
    position: relative;
    overflow: visible;
}
