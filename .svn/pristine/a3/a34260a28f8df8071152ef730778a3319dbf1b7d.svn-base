@charset "UTF-8";
	html{
		cursor: default;
	    line-height: 1.5;
	    -moz-tab-size: 4;
	    tab-size: 4;
	    -webkit-tap-highlight-color: transparent;
	    -ms-text-size-adjust: 100%;
	    -webkit-text-size-adjust: 100%;
	    word-break: break-word;
	}
	body{
		background-color: rgb(250, 251, 252);
	    color: rgb(32, 36, 41);
	    font-family: "Noto Sans KR", sans-serif;
	    font-size: 14px;
	    font-weight: normal;
	    font-style: normal;
	    font-stretch: normal;
	    line-height: 1.4;
	    letter-spacing: normal;
	    width: 100%;
	    height: 100%;
	}
	div {
    	display: block;
	}
	aside {
    	display: block;
	}
	section{
    	display: block;
	}
	ul {
	    display: block;
	    list-style-type: disc;
	    margin-block-start: 1em;
	    margin-block-end: 1em;
	    margin-inline-start: 0px;
	    margin-inline-end: 0px;
	    padding-inline-start: 40px;
	}
	a {
	    color: inherit;
	    text-decoration: none;
	    cursor: pointer;
	}
	.header{
		display: block;
	    width: 100%;
	    height: 75px;
	}
	.page{
		display: flex;
	    position: relative;
	    width: 1216px;
	    padding-left: 48px;
	    padding-right: 48px;
	    margin: 0px auto;
	    padding-top: 40px;
	    background-color: rgb(250, 251, 252);
	}
	.left{
		width: 240px;
	}
	.center{
		width: 880px;
	    position: relative;
	    background-color: rgb(250, 251, 252);
	}
	.outer{
		padding: 0px 35px;
	    background-color: rgb(255, 255, 255);
	    border: 1px solid rgb(235, 235, 235);
	    border-radius: 2px;
	}
	.inner{
		position: relative;
	    overflow: hidden;
	    padding-bottom: 32px;
	}
	.inner-else{
		border-top: 1px solid rgb(235, 235, 235);
    	padding-top: 12px;
	}
	.category{
		display: flex;
	    -webkit-box-align: center;
	    align-items: center;
	    flex-direction: row;
	    height: 64px;
	}
	.categoryname{
		color: rgb(32, 36, 41);
	    font-size: 16px;
	    font-weight: 500;
	    line-height: 1.5;
	    padding: 0px;
	    margin: 0px;
	}
	.showAll{
		color: rgb(173, 177, 184);
    	margin-left: auto;
	}
	.list{
		padding: 0px;
	    list-style: none;
	    margin: 0px;
	    width: 824px;
	}
	.list-video{
		outline: none;
    	width: 206px;
	    float: left;
	    height: 100%;
	    min-height: 1px;
	}
	.cont-video{
		width: 187px;
    	margin: 0px;
	}
	.thumb{
		height: 105px;
	    border-radius: 4px;
	    width: 100%;
	    overflow: hidden;
	    margin-bottom: 8px;
	    position: relative;
	}
	.thumb-img{
		background-position: center center;
	    background-repeat: no-repeat;
	    background-size: cover;
	    background-color: rgb(235, 235, 235);
	    height: 100%;
	    outline: rgba(0, 0, 0, 0.1) solid 1px;
	    outline-offset: -1px;
	}
	.title{
		display: -webkit-box;
	    -webkit-line-clamp: 2;
	    -webkit-box-orient: vertical;
	    overflow: hidden;
	    line-height: 20px;
	    max-height: 40px;
	    margin-bottom: 4px;
	}
	.date{
		display: inline-flex;
	    -webkit-box-align: center;
	    align-items: center;
	    color: rgb(173, 177, 184);
	    font-size: 12px;
	}
	.footer{
		margin-top: 100px;
	    padding-bottom: 40px;
	    font-size: 13px;
	    text-align: center;
	}
	.fout{
		margin-top: 4px;
	    display: flex;
	    -webkit-box-pack: center;
	    justify-content: center;
	    -webkit-box-align: center;
	    align-items: center;
	}
	.fin{
		color: rgb(135, 142, 150);
    	margin-top: 20px;
	}