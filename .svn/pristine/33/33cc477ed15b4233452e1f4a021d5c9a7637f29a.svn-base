.list-video{
	outline: none;
   	width: 206px;
    float: left;
    height: 100%;
    min-height: 1px;
}
.cont-video{
	width: 187px;
   	margin: 0px;
}
.thumb{
	height: 105px;
    border-radius: 4px;
    width: 100%;
    overflow: hidden;
    margin-bottom: 8px;
    position: relative;
}
.thumb-img{
	background-position: center center;
    background-repeat: no-repeat;
    background-size: cover;
    background-color: rgb(235, 235, 235);
    height: 100%;
    outline: rgba(0, 0, 0, 0.1) solid 1px;
    outline-offset: -1px;
}
.title{
	display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    line-height: 20px;
    max-height: 40px;
    margin-bottom: 4px;
}
.date{
	display: inline-flex;
    -webkit-box-align: center;
    align-items: center;
    color: rgb(173, 177, 184);
    font-size: 12px;
}
.footer{
	margin-top: 100px;
    padding-bottom: 40px;
    font-size: 13px;
    text-align: center;
}
.fout{
	margin-top: 4px;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
}
.fin{
	color: rgb(135, 142, 150);
   	margin-top: 20px;
}
.fcolor{
	color : #2C7CE6; 
}
.aside1{
	width: 240px;
