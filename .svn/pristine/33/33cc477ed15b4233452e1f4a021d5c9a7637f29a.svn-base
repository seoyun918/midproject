.category{
	display: flex;
    -webkit-box-align: center;
    align-items: center;
    flex-direction: row;
    height: 64px;
}
.categoryname{
	color: rgb(32, 36, 41);
    font-size: 16px;
    font-weight: 500;
    line-height: 1.5;
    padding: 0px;
    margin: 0px;
}
.showAll{
	color: rgb(173, 177, 184);
   	margin-left: auto;
}
