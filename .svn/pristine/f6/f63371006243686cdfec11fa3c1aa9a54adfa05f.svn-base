<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>QnA</title>
<style>
*, :after, :before{
	box-sizing: border-box;
}
div {
    display: block;
}
a{
	color: inherit;
	text-decoration: none;
}
.qna1{
	margin: 0px auto;
	display: flex;
	flex-direction: column;
	width: 896px;
	padding: 0px 48px;
}
.qnaname{
	display: block;
    font-size: 1.5em;
    margin-block-start: 0.83em;
    margin-block-end: 0.83em;
    margin-inline-start: 0px;
    margin-inline-end: 0px;
    font-weight: bold;
}

iframe{
	padding: 0px;
	margin: 30px 0px 0px;
	list-style: none;
	min-height: 570px;
	border: 1px solid lightgray;
	border-radius: 8px;
}
.qnamenu{
	margin: 0px;
	padding: 0px;
}
li{
	display: inline;
	margin: 0px;
	padding: 0px;
}
</style>
</head>
<body>
<div class="qna1">
	<h2 class="qnaname">문의</h2>
	<ul class="qnamenu">
		<li><a href="">자주묻는질문</a></li>
		<li><a href="">1:1문의하기</a></li>
		<li><a href="">내가 쓴 문의</a></li>
	</ul>
	<iframe src="" name="main" scrolling="no" frameborder="0"></iframe>
</div>
	
	
	



</body>
</html>