package soju.vo;

public class AlramVO {
	private int alr_num;
	private String alr_lgu;
	private String alr_cont;
	private int mem_num;
	
	public int getAlr_num() {
		return alr_num;
	}
	public void setAlr_num(int alr_num) {
		this.alr_num = alr_num;
	}
	public String getAlr_lgu() {
		return alr_lgu;
	}
	public void setAlr_lgu(String alr_lgu) {
		this.alr_lgu = alr_lgu;
	}
	public String getAlr_cont() {
		return alr_cont;
	}
	public void setAlr_cont(String alr_cont) {
		this.alr_cont = alr_cont;
	}
	public int getMem_num() {
		return mem_num;
	}
	public void setMem_num(int mem_num) {
		this.mem_num = mem_num;
	}
}
